<launch>
<node pkg="explore_lite" type="explore" respawn="false" name="explore" output="screen">
  <param name="robot_base_frame" value="X1"/>
  <param name="costmap_topic" value="/X1/move_base/global_costmap/costmap"/>
  <param name="costmap_updates_topic" value="/X1/move_base/global_costmap/costmap_updates"/>
  <param name="visualize" value="true"/>
  <param name="planner_frequency" value="0.25"/>
  <param name="progress_timeout" value="5.0"/>
  <param name="potential_scale" value="100.0"/>
  <param name="orientation_scale" value="0.8"/>
  <param name="gain_scale" value="50.0"/>
  <param name="transform_tolerance" value="0.3"/>
  <param name="min_frontier_size" value="1.5"/>
</node>
</launch>
